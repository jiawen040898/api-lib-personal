{
    "name": "@pulsifi/api",
    "version": "4.8.0",
    "description": "An api lib repository that contains all nestjs api common use stuffs",
    "repository": {
        "type": "git",
        "url": "https://github.com/Pulsifi/api-lib"
    },
    "keywords": [
        "api"
    ],
    "author": "Pulsifi <dev-team@pulsifi.me>",
    "license": "UNLICENSED",
    "main": "index.js",
    "typings": "./types/index.d.ts",
    "scripts": {
        "copy": "copyfiles package.json ./lib & copyfiles -u 3 ./src/configs/dev/**/*.* ./lib/configs/dev",
        "prepare": "is-ci || husky",
        "prebuild": "rimraf lib",
        "postversion": "cp -r package.json ..",
        "build": "tsc --project tsconfig.production.json && yarn copy",
        "format": "prettier --log-level \"warn\" --write \"{src,test}/**/*.ts\"",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "test": "jest --watch --maxWorkers=50%",
        "test:ci": "jest --ci --coverage --maxWorkers=50%"
    },
    "dependencies": {
        "@aws-sdk/client-sns": "3.516.0",
        "@aws-sdk/client-sqs": "3.515.0",
        "@google-cloud/bigquery": "7.4.0",
        "@google-cloud/pubsub": "4.3.2",
        "@nestjs/axios": "3.0.2",
        "@nestjs/cache-manager": "2.2.1",
        "@nestjs/common": "10.3.3",
        "@nestjs/config": "3.2.0",
        "@nestjs/core": "10.3.3",
        "@nestjs/swagger": "7.3.0",
        "@nestjs/typeorm": "10.0.2",
        "@pulsifi/auth": "2.3.0",
        "@sentry/integrations": "7.102.0",
        "@sentry/node": "7.102.0",
        "auth0": "3.7.2",
        "axios": "1.6.7",
        "cache-manager": "5.4.0",
        "cache-manager-redis-yet": "4.1.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "generate-password": "1.7.1",
        "i18next": "23.9.0",
        "i18next-http-backend": "2.4.3",
        "liquidjs": "10.10.1",
        "lodash": "4.17.21",
        "moment-timezone": "0.5.45",
        "nestjs-pino": "4.0.0",
        "pino-http": "9.0.0",
        "pusher": "5.2.0",
        "rxjs": "7.8.1",
        "typeorm": "0.3.20",
        "unleash-client": "5.5.1",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "18.6.1",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/npm": "11.0.2",
        "@types/auth0": "3.3.10",
        "@types/cache-manager": "4.0.6",
        "@types/jest": "29.5.12",
        "@types/lodash": "4.14.202",
        "@types/node": "20.11.19",
        "@types/uuid": "9.0.8",
        "@types/validator": "13.11.9",
        "@typescript-eslint/eslint-plugin": "7.0.2",
        "@typescript-eslint/parser": "7.0.2",
        "commitlint-plugin-function-rules": "3.1.0",
        "copyfiles": "2.4.1",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-simple-import-sort": "12.0.0",
        "husky": "9.0.11",
        "is-ci": "3.0.1",
        "jest": "29.7.0",
        "jest-junit": "16.0.0",
        "lint-staged": "15.2.2",
        "prettier": "3.2.5",
        "rimraf": "5.0.5",
        "semantic-release": "23.0.2",
        "ts-jest": "29.1.2",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "typescript": "5.3.3"
    }
}
